{
  "name": "Main Adblock Blocklist",
  "description": "Optimized adblock format blocklist from multiple sources with advanced filtering",
  "homepage": "https://github.com/AdguardTeam/HostlistCompiler",
  "license": "GPLv3",
  "version": "1.0.0.0",
  "sources": [
    {
      "name": "AdGuard DNS Filter",
      "source": "https://adguardteam.github.io/AdGuardSDNSFilter/Filters/filter.txt",
      "type": "adblock",
      "transformations": ["RemoveComments", "RemoveModifiers", "Validate"]
    },
    {
      "name": "Local Adblock Rules",
      "source": "sources/local-rules.txt",
      "type": "adblock",
      "transformations": ["RemoveComments", "Validate"]
    }
  ],
  "transformations": ["Deduplicate", "Compress", "Validate", "RemoveEmptyLines", "TrimLines", "InsertFinalNewLine"]
}